@model AccountTransactionViewModel

<div id="accountDetailsDiv">
    <label asp-for="@Model.Account.AccountId">Kontonummer</label>
    <p asp-for="@Model.Account.AccountId">@Model.Account.AccountId</p>
    <label asp-for="@Model.Account.Balance">Saldo</label>
    <p asp-for="@Model.Account.Balance">@Model.Account.Balance</p>
    <br />
    <a asp-controller="Account" asp-action="Deposit" asp-route-id="@Model.Account.AccountId">Insättning</a>
    <a asp-controller="Account" asp-action="Withdrawal" asp-route-id="@Model.Account.AccountId">Uttag</a>
    <a asp-controller="Account" asp-action="Transaction" asp-route-id="@Model.Account.AccountId">Överföring</a>
    <br /><br />
    <h2>Transaktioner</h2>
    <table>
        <tr>
            <th>Transaktions id</th>
            <th>Datum</th>
            <th>Typ av åtgärd</th>
            <th>Procedur</th>
            <th>Summa</th>
            <th>Nytt saldo</th>
            <th>Anteckning</th>
            <th>Bank</th>
            <th>Till/från konto</th>
        </tr>
        <partial name="_TransactionRows" />
    </table>

</div>